{
  "name": "aspian-dashboard",
  "version": "1.0.0",
  "description": "ASPIAN dashboard",
  "main": "index.html",
  "scripts": {
    "i18n-watch": "watch 'npm run i18n' src/templates src/locales",
    "i18n": "static-i18n -l en -i fa -i en src/templates --localesPath src/locales/ -o src/i18n",
    "webpack-dev": "webpack-dev-server --open --config=config/webpack.dev.js",
    "start": "npm-run-all --parallel webpack-dev i18n-watch",
    "build-ltr": "webpack --config=config/webpack.prod.js",
    "build-rtl": "webpack --config=config/webpack.prod.rtl.js",
    "build": "npm-run-all --parallel build-ltr build-rtl"
  },
  "author": "Omid Rouhani",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.4",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "watch": "^1.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "last 10 versions",
    "> 0.5%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 8",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@joeattardi/emoji-button": "^2.12.1",
    "bootstrap": "^4.4.1",
    "bootstrap-v4-rtl": "^4.4.1-1",
    "dropzone": "^5.7.0",
    "jalali-moment": "^3.3.3",
    "jquery": "^3.4.1",
    "perfect-scrollbar": "^1.5.0",
    "popper.js": "^1.16.1",
    "typeface-poppins": "0.0.72",
    "vazir-font": "^24.1.0"
  }
}
